d_" freeze_columns="0"><sort/><column_widths><column index="1" value="84"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="rename_caneloB" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="54"/><column index="2" value="85"/><column index="3" value="84"/><column index="4" value="66"/><column index="5" value="54"/><column index="6" value="54"/><column index="7" value="101"/><column index="8" value="41"/><column index="9" value="75"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="Analysis*">--Ananlysis for punches landed 
SELECT fighter, SUM(CASE WHEN landed = 'landed' THEN 1 ELSE 0 END) AS total_punches_landed
FROM(
    SELECT fighter, landed FROM rename_caneloB
    UNION ALL
    SELECT fighter, landed FROM rename_caneloG
    UNION ALL
    SELECT fighter, landed FROM rename_usyk1
    UNION ALL
    SELECT fighter, landed FROM rename_usyk2
) AS Combined
WHERE fighter IN ('Canelo','Usyk')
GROUP BY fighter;

--Analysis for punches missed
SELECT fighter, SUM(CASE WHEN landed = 'missed' THEN 1 ELSE 0 END) AS total_punches_missed
FROM combined_fighters
WHERE fighter IN ('Canelo','Usyk')
GROUP BY fighter;

-- Punches missed vs punches landed
SELECT fighter, SUM(CASE WHEN landed = 'landed' THEN 1 ELSE 0 END) AS total_punches_landed, 
SUM(CASE WHEN landed = 'missed' THEN 1 ELSE 0 END) AS total_punches_missed
FROM combined_fighters
WHERE fighter IN ('Canelo','Usyk')
GROUP BY fighter;

--Percentage of power punches thrown vs jabs
SELECT 
  fighter,
  COUNT(CASE WHEN punch_type = 'powerpunch' THEN 1 END) * 100.0 / COUNT(*) AS percentage_powerpunch, 
  COUNT(CASE WHEN punch_type = 'jab' THEN 1 END) * 100.0 / COUNT(*) AS percentage_jab 
FROM combined_fighters
WHERE fighter IN ('Canelo','Usyk')
GROUP BY fighter;

--Canelo number of punches landed per round
SELECT fighter, round, SUM(CASE WHEN landed = 'landed' THEN 1 ELSE 0 END) AS landed_punches 
FROM combined_fighters 
WHERE fighter IN ('Canelo') 
GROUP BY round 
ORDER BY round;

--Usyk number of punches landed per round
SELECT fighter, round, SUM(CASE WHEN landed = 'landed' THEN 1 ELSE 0 END) AS landed_punches 
FROM combined_fighters 
WHERE fighter IN ('Usyk') 
GROUP BY round 
ORDER BY round;

--Count of combo vs single punches thrown
SELECT fighter, SUM(CASE WHEN combo = 'Combo' THEN 1 ELSE 0 END) AS 'Combo Punches Landed', 
SUM(CASE WHEN combo = 'Single' THEN 1 ELSE 0 END) AS 'Single Punches Landed'
FROM combined_fighters 
WHERE fighter IN ('Usyk','Canelo')
GROUP BY fighter;

--Number or Counters landed
SELECT fighter, SUM(CASE WHEN counter_punch = 'counterpunch' THEN 1 END) 
	AS 'Counter Punches Landed' 
FROM combined_fighters
WHERE fighter IN ('Usyk','Canelo')
GROUP BY fighter;

--Landed Punches by Placement, Canelo
SELECT placement, COUNT(*) AS landed_punches 
FROM combined_canelo
WHERE landed = 'landed' 
GROUP BY Placement;

--Landed Punches by Placement, Usyk
SELECT placement, COUNT(*) AS landed_punches 
FROM combined_usyk
WHERE landed = 'landed' 
GROUP BY Placement;

--Number of Each Type of Punch Thrown, Canelo
SELECT punch_type, COUNT(*) AS count 
FROM combined_canelo 
WHERE fighter = 'Canelo'
GROUP BY punch_type; 

--Number of Each Type of Punch Thrown, Usyk
SELECT punch_type, COUNT(*) AS count 
FROM combined_usyk
WHERE punch_type IN ('hold','jab','powerpunch')
	AND fighter = 'Usyk'
GROUP BY punch_type;

-- </sql><sql name="created views">CREATE VIEW combined_fighters AS
SELECT * FROM rename_caneloB 
UNION ALL 
SELECT * FROM rename_caneloG 
UNION ALL 
SELECT * FROM rename_usyk1 
UNION ALL 
SELECT * FROM rename_usyk2;

--Combined canelo fights
CREATE VIEW combined_canelo AS
SELECT * FROM rename_caneloB 
UNION ALL 
SELECT * FROM rename_caneloG ;

--Combined usyk fights
CREATE VIEW combined_usyk AS
SELECT * FROM rename_usyk1 
UNION ALL 
SELECT * FROM rename_usyk2;
</sql><current_tab id="0"/></tab_sql></sqlb_project>
